{
  "version": 2,
  "buildCommand": "cd breast-cancer-ui && npm run build",
  "outputDirectory": "breast-cancer-ui/.next",
  "installCommand": "cd breast-cancer-ui && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/vercel_handler.py"
    }
  ],
  "functions": {
    "api/vercel_handler.py": {
      "runtime": "python3.9",
      "maxDuration": 30,
      "memory": 3008
    }
  }
}

